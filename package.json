{
  "name": "vestledger-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lighthouse:auth": "lhci autorun --config=./lighthouserc.auth.json && node scripts/lhci-append-assertions.js auth",
    "lighthouse:public": "lhci autorun --config=./lighthouserc.public.json && node scripts/lhci-append-assertions.js public",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui",
    "playwright:headed": "playwright test --headed",
    "playwright:debug": "playwright test --debug",
    "playwright:report": "playwright show-report",
    "prepare": "husky",
    "gen:openapi": "node scripts/gen-openapi.mjs"
  },
  "dependencies": {
    "@cyntler/react-doc-viewer": "^1.17.1",
