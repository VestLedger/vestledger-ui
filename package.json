    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "babel-plugin-istanbul": "^7.0.1",
    "cypress": "^15.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "jsdom": "^25.0.1",
