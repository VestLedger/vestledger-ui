    "@reduxjs/toolkit": "^2.11.1",
    "@tanstack/react-table": "^8.20.5",
    "canvas": "^3.2.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.0",
    "lucide-react": "^0.487.0",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.15.0",
    "pdfjs-dist": "3.11.174",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-medium-image-zoom": "^5.4.0",
